#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = KBEditor.exe KBEditor.RUS
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

KBEditor.exe: KBEditor.dpr
  $(DCC)

KBEditor.RUS: RUS\KBEditor.dpr
  $(DCC)


